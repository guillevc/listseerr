    "@tanstack/router-devtools": "^1.139.12",
    "@tanstack/router-vite-plugin": "^1.139.12",
    "@types/bun": "^1.1.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
